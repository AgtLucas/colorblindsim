{
  "name": "colorblind",
  "version": "1.0.0",
  "main": "script.js",
  "scripts": {
    "watch": "parcel src/index.html",
    "watch:https": "parcel --https src/index.html",
    "build":
      "rm -rf dist && parcel build --no-cache --public-url './' src/index.html --detailed-report && svgo dist/*.svg && ./scripts/prerender.js && workbox generateSW",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "author": "Koen Kivits <koen@kivits.com>",
  "license": "UNLICENSED",
  "private": true,
  "keywords": [],
  "description": "",
  "dependencies": {
    "node-sass": "^4.7.2",
    "preact": "^8.2.7",
    "preact-feather": "^2.0.1",
    "unistore": "^3.0.4"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-preact": "^1.1.0",
    "babel-preset-stage-3": "^6.24.1",
    "husky": "^0.14.3",
    "jsdom": "^11.6.2",
    "lint-staged": "^6.1.0",
    "parcel-bundler": "^1.6.2",
    "parcel-plugin-shaders": "file:lib/parcel-plugin-shaders",
    "prettier": "^1.10.2",
    "svgo": "^1.0.5",
    "workbox-build": "^3.0.0",
    "workbox-cli": "^3.0.0"
  }
}
